! vim:ft=xdefaults:

!!! urxvt
URxvt.termName:                   xterm-256color
URxvt.font:                       xft:Cascadia Mono:size=10
URxvt.geometry:                   75x28
URxvt.internalBorder:             30
URxvt.cursorUnderline:            true
URxvt.scrollBar:                  false
URxvt.scrollstyle:                plain
URxvt.pointerBlank:               true
URxvt.depth:                      24
URxvt.iso14755:                   false
URxvt.iso14755_52:                false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
!!! colors
*.foreground: #f0f0f0
*.background: #0f0f0f

*.color0:     #262626
*.color8:     #4c4c4c
*.color1:     #ac8a8c
*.color9:     #c49ea0
*.color2:     #8aac8b
*.color10:    #9ec49f
*.color3:     #aca98a
*.color11:    #c4c19e
*.color4:     #8f8aac
*.color12:    #a39ec4
*.color5:     #ac8aac
*.color13:    #c49ec4
*.color6:     #8aabac
*.color14:    #9ec3c4
*.color7:     #e7e7e7
*.color15:    #f5f5f5

!!! xft
Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault
