! vim:ft=xdefaults:

!!! Xterm
XTerm.vt100.faceName: xft:Cascadia Mono:size=12
XTerm*geometry: 66x22
XTerm*internalBorder: 40
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm*renderFont: true
XTerm.vt100.translations: #override \n\
    Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
    Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

!!! colors
*background: #282828
*foreground: #d4be98

*color0:  #32302f
*color8:  #32302f
*color1:  #ea6962
*color9:  #ea6962
*color2:  #a9b665
*color10: #a9b665
*color3:  #e78a4e
*color11: #e78a4e
*color4:  #7daea3
*color12: #7daea3
*color5:  #d3869b
*color13: #d3869b
*color6:  #89b482
*color14: #89b482
*color7:  #d4be98
*color15: #d4be98

!!! xft
Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault
